{
    "name": "govcms/installer",
    "description": "Composer-based installer for the govCMS Drupal distribution",
    "keywords": ["govCMS"],
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "ext-zip": "*",
        "guzzlehttp/guzzle": "~4.0|~5.0|~6.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "govCMS\\Installer\\Console\\": "src/"
        }
    },
    "bin": ["gc"],
    "conflict": {
        "acquia/blt": "*",
        "drupal/core": "7.*"
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
